    tft.drawFastHLine(0, 44, 440, 0x0000);
    tft.drawFastHLine(0, 45, 440, 0x0000);
    tft.setCursor(LCol1, LLine1);
    tft.print("Setpoint ");
    tft.setCursor(LCol1, LLine2);
    tft.print("Actual ");
  
    tft.setFont(&FreeSans9pt7b);
    tft.setTextSize(2);
    tft.setCursor(LCol2, LLine1);
    tft.print(Setpoint, 1);
    tft.setCursor(LCol2, LLine2);
    tft.print(Temperature, 1);
  
    last_temperature = Temperature;
    last_Setpoint = Setpoint;
  
    Serial.println();
  
    tft.setCursor(240, 220);
    tft.setTextColor(txt_clr);
    tft.setFont(&FreeSans9pt7b);
    tft.print("Off");

    PID_Ticks = 0;
    print_datalog_header();
    State = Run;

    // ********************************* main loop ***********************************
    
    while (true){                     // This is main loop
        tft.setFont(&FreeSans9pt7b);
        int down = 0;
        int held = 0;
        double multi = 1;
        if (digitalRead(PowerButton) == 0){     // Test for On/Off button press
            analogWrite(FET_Pin,0); 
            tft.fillScreen(0x0000);         // ack press
            while (digitalRead(PowerButton) == 0){  // wait for release
              
            }
            turnOff();
            reDrawMain();
            PID_Ticks=0;
        }

        while((digitalRead(PB1) == 0) && (digitalRead(PB2) == 0)){
            if(held > 3000){
                State = Edit;
                Serial.println("Edit detected");
                analogWrite(FET_Pin,0); 
                Edit_Parameters();
                refresh = 1;
                PID_Ticks = 0;
               Serial.println("Done with edit");
               State = Run;
            }else{
                held += 50;
                delay_WDT(50);
            }
           // refresh = 1;
          //  PID_Ticks = 0;
           
            continue;
        }
    
        while((digitalRead(PB1) == 0) and set_on){
            if (Setpoint <= 525) {
                analogWrite(FET_Pin,0); 
                tft.setCursor(LCol2, LLine1);
                tft.setTextColor(bg_color);
                tft.print(Setpoint,1);
                Setpoint += (0.1) * multi;
                tft.setCursor(LCol2, LLine1);
                tft.setTextColor(txt_clr);
                tft.print(Setpoint,1);
                Write_NVM();
                if (down++ < 9)
                    delay_WDT(200);
                else if(down++ < 18){
                    multi = 10;
                    delay_WDT (200);
                }else{
                    multi = 50;
                    delay_WDT(200);
                }
            }
        }
        while((digitalRead(PB2) == 0) && set_on){
            if (Setpoint >= 0) {
                analogWrite(FET_Pin,0); 
               
                tft.setCursor(LCol2, LLine1);
                tft.setTextColor(bg_color);
                tft.print(Setpoint,1);
                Setpoint -= (0.1) * multi;
                Write_NVM();
                tft.print(Setpoint,1);
                tft.setCursor(LCol2, LLine1);
                tft.setTextColor(txt_clr);
                tft.print(Setpoint,1);
                if (down++ < 10)
                    delay_WDT(100);
                else if(down++<20){
                    multi = 10;
                    delay_WDT (100);
                }else{
                    multi = 50;
                    delay_WDT(100);
                }
            }
        }
        if (digitalRead(PB3) == 0){
            if(set_on){
              //  State = Off;
                Serial.println("**** User Hit Off Key ****");
                analogWrite(FET_Pin,0); 
                set_rem = Setpoint;
                last_Setpoint = 0;
                Setpoint = 0;
                set_on = 0;
                tft.setFont(&FreeSans9pt7b);
                tft.setTextColor(bg_color);  tft.setTextSize(2);
                tft.setCursor(LCol2, LLine1);
                tft.print(set_rem, 1);
                tft.setCursor(240, 220);
                tft.print("Off");
                tft.setTextColor(txt_clr);
                tft.setCursor(240, 220);
                tft.print("On");
                tft.setCursor(LCol2, LLine1);
                tft.print("Off");
                while (digitalRead(PB3) == 0){  // wait for it to go up
                  
                }
                delay_WDT(333);
                continue;
            }else{
             //   State = On;
                Serial.println("**** User Hit On Key ****");
                analogWrite(FET_Pin,0); 
                Setpoint = set_rem;
                last_Setpoint = Setpoint;
                tft.setFont(&FreeSans9pt7b);
                set_on = 1;
                tft.setCursor(LCol2, LLine1);
                tft.setTextColor(bg_color); 
                tft.print("Off");
                tft.setCursor(240, 220);
                tft.print("On");
                tft.setTextColor(txt_clr);
                tft.setCursor(240, 220);
                tft.print("Off");
                tft.setCursor(LCol2, LLine1);
                tft.setTextColor(txt_clr);
                tft.print(Setpoint, 1);
                while (digitalRead(PB3) == 0){  // wait for it to go up
                  
                }
                delay_WDT(333);
                continue;
            }
        }

        if (fault != 0){         // check and see if we had a fault while editting setpoint or parameters
            Break_Code = 7;
            break;
        }

       strobe_WDT();
       
        if (RTD_Fault != 0){ 
          Break_Code = 2;
          break;
        }
   
       Run_PID(); 
       Int_Out = Output;
       analogWrite(FET_Pin, Int_Out);             // Write PID output to heater     
 
        Test_Heater();                            // Check and see if heater current is in tolerance
            if (HTR_Fault != 0){
            Break_Code = 3;
            break;
        }

        if (PID_Ticks == 1)    // check heater every other cycle
           {
                Current = analogRead(0); 
                if (Current < 500)           // check for over current (value gets lower as current increases
                    {
                      Break_Code = 3;
                      HTR_Fault = 1;        
                    //  break;
                    }
                if (Int_Out > 100)          // test for under current (only if we are driving heater at a reasonable level)
                {
                    if (Current > 600)      // 650-635 is zero current, 600 approx PWM 75, at PWM 100 we should see approx 580     
                        {
                          Break_Code = 3;
                          HTR_Fault = 2;        // under
                        //  break;
                        } 
                }
               // print_debug_line();
                if (State == Run)
                  print_datalog_data();    // only print if we are running
           
           }  // PID_Ticks == 1

        if (PID_Ticks == 2)
           {
               analogWrite(FET_Pin,0);  // turn off PWM and heater
               strobe_WDT();
               delay(5);
               read_temp();     // read the temp
               if(RTD_Fault == 4)  // Look for RTD noise fault
                                {
                                    Serial.println("****");
                                    Serial.println("Warning ... RTD Fault = 4 .. Under/Over voltage on inputs");
                                    RTD_Fault = 0;  // reset fault  
                                    Temperature = last_temperature;  // use the last good temperature
                                    Fault_Count++;
                                    Serial.print("FC = ");
                                    Serial.println(Fault_Count);
                                    Serial.print("Time = ");
                                    Serial.println(now / 1000, 1);
                                    Serial.println();
                                    
                                }
               
              if ((Temperature != last_temperature) || (refresh == 1)){       // Update TFT only if data has changed to reduce flicker
                
                 
                //  Serial.print("1 refresh = ");Serial.println(refresh);
                  tft.setTextColor(bg_color);  tft.setTextSize(2);
                  tft.setCursor(LCol2, LLine2);
                  tft.print(last_temperature, 1);
                  tft.setTextColor(txt_clr);  tft.setTextSize(2);
                  tft.setCursor(LCol2, LLine2);
                  tft.print(Temperature, 1);
                  last_temperature = Temperature;

                  if(Output > 0){
                      tft.setCursor(LCol2-20, LLine2); 
                      tft.print("*");
                  }else{
                      tft.setCursor(LCol2-20, LLine2);
                      tft.setTextColor(bg_color);
                      tft.print("*");
                      tft.setTextColor(txt_clr);
                      }
      
                } // if temp != last temp || (refresh == 1)
      
              if ((Setpoint != last_Setpoint)){      // Update TFT only if data has changed to reduce flicker
                  analogWrite(FET_Pin,0); 
               //   Serial.print("2 refresh = ");Serial.println(refresh);
                  tft.setTextColor(bg_color);  tft.setTextSize(2);
                  tft.setCursor(LCol2, LLine1);
                  tft.print(last_Setpoint, 1);
                  tft.setTextColor(txt_clr);  tft.setTextSize(2);
                  tft.setCursor(LCol2, LLine1);
                  tft.print(Setpoint, 1);
                  last_Setpoint = Setpoint;
                  
              }
              PID_Ticks = 0;
              refresh = 0;
              analogWrite(FET_Pin,Int_Out);
           }

    }  // main loop bracket

  // To get here, we bailed from main loop above because PB was pressed or we had a fault

FLT_Bail:

    analogWrite(FET_Pin, 0);        // Turn Off Heater
  
    Serial.print("Break Code = ");
    Serial.println(Break_Code);
  
    switch (Break_Code) {
        case 1:
            Serial.println("PB Pressed .... Shutdown");
            break;
      case 2:
            Serial.print("RTD Fault ..."); Serial.println(RTD_Fault);
            break;
      case 3:
            Serial.print("Heater Fault = "); Serial.println(HTR_Fault);
            break;
    }
    tft.fillScreen(bg_color);
    tft.setCursor(0, 0);
    tft.setTextColor(ILI9341_WHITE);  tft.setTextSize(3);
    tft.fillScreen(bg_color);
    tft.setCursor(0, 0);
    Setpoint = 0;
    goto SU2;
}
