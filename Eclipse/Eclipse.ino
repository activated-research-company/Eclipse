void initialize() {
  
  Serial.begin(9600);
  
  tft.begin();
  tft.fillScreen(color_black);
  
  max.begin(MAX31865_2WIRE); // RTD code and RTD Type
  
  SetControllerDirection(DIRECT);
  SetSampleTime(500); // loop rate (500 = 2 Hz)
  SetOutputLimits(00, 255); // min and max for output
  SetMode(AUTOMATIC);
  SetTunings(2, .5, 0); // initial tuning parameters - P, I, D
  
  pinMode(LEDA, OUTPUT);
  pinMode(WDT, OUTPUT);
  pinMode(FET_Pin, OUTPUT);
  
  InFilt_State = 0;
  OutFilt_State = 0; // int filter states, NVM recall will override later
  
  tft.fillScreen(color_black);
  
}

void turnOff(){
  State = Sleep;
  tft.fillScreen(color_black);
  set_rem = Setpoint;
  Setpoint = 0.0;
  waitToBeTurnedOn();
}

void waitToBeTurnedOn() {

  while (digitalRead(PowerButton) == 0) {
    //wait for power button to be a non-depressed state
  }

  i = 0;
  bool buttonPressComplete = false;
  
  while (digitalRead(PowerButton) == 1) { // button is not depressed

    analogWrite(LEDA, i); // PWM the A side
    
    if (i == 255) x = -1;
    if (i == 0) x = 1;
    i = i + x;
    
    delay_WDT(10);

    while (digitalRead(PowerButton) == 0) { // button is depressed
        buttonPressComplete = true;
    }
    if (buttonPressComplete) {
      break;
    }
  }

  turnOn();
  
}

void turnOn() {
  digitalWrite (LEDA, HIGH);
  Setpoint = set_rem;
  refresh = 1;
  State = Run;
}

void strobe_WDT()
{
    WDT_State = !WDT_State;            // Toggle WDT output variable
    digitalWrite (WDT , WDT_State);    //Write to strobe pin
}

void delay_WDT(int ms)   // use this instaed of delay function to prevent WDT time out
{
   int ticks;
   for (ticks = 0; ticks < ms; ticks++)
   {
      delay(1);
      strobe_WDT();
   }
}

